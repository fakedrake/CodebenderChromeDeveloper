{
    "name": "CodebenderDev",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Development of codebender.cc tools",
    "background": {
	"scripts": ["redirect.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://staging.codebender.cc/*"],
            "js": ["content.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "web_accessible_resources": ["bundles/*"],
    "permissions": [
        "<all_urls>", "management", "activeTab",
        "webRequest", "webRequestBlocking"
    ]
}
